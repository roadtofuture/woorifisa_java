	@Autowired
	@Qualifier("iman")
	IronMan iman;
	
	@Test
	public void ironManTest() {
		Assertions.assertNotNull(iman);
	}

	@Test
	public void beanTest() {
		Assertions.assertNotNull(hulk);
		hulk.help();
	}
		
}
